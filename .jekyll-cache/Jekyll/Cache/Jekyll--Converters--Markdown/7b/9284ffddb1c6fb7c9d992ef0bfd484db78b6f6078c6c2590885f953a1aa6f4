	<span class="nc">Car</span><span class="o">(</span><span class="nc">String</span> <span class="n">c</span><span class="o">,</span> <span class="nc">String</span> <span class="n">g</span><span class="o">,</span> <span class="kt">int</span> <span class="n">d</span><span class="o">)</span> <span class="o">{</span> <span class="c1">// л§¤к°њліЂм€к°Ђ мћ€лЉ” мѓќм„±мћђ</span>
