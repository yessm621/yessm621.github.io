	<span class="nc">String</span> <span class="n">color</span><span class="o">;</span> <span class="c1">// мѓ‰мѓЃ</span>
	<span class="nc">String</span> <span class="n">gearType</span><span class="o">;</span> <span class="c1">// ліЂм†Ќкё° мў…лҐ - auto(мћђлЏ™), manual(м€лЏ™)</span>
