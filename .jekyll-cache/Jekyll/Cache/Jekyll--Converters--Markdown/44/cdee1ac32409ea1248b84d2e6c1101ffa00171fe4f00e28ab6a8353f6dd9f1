<span class="o">}</span>

<span class="kd">class</span> <span class="nc">Parent</span> <span class="o">{</span>
	<span class="kt">int</span> <span class="n">x</span> <span class="o">=</span> <span class="mi">10</span><span class="o">;</span> <span class="cm">/* super.x */</span>
